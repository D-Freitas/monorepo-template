{
  "name": "monorepo-template",
  "author": "JusDocs",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9.6.0"
  },
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "build": "npx turbo run build --cache-dir='./cache'",
    "build:all": "npx turbo run build --cache-dir='./cache' --force",
    "start": "npx turbo run start",
    "test": "npx turbo run test --cache-dir='./cache'",
    "test:all": "npx turbo run test --cache-dir='./cache' --force",
    "fix": "npx turbo run fix --cache-dir='./cache'",
    "fix:all": "npx turbo run fix --cache-dir='./cache' --force",
    "lint": "npx turbo run lint --cache-dir='./cache'",
    "lint:all": "npx turbo run lint --cache-dir='./cache' --force"
  },
  "dependencies": {
    "@monorepo-template/shared": "workspace:*"
  },
  "devDependencies": {
    "@monorepo-template/config": "workspace:*",
    "eslint": "9.35.0",
    "eslint-plugin-jest": "29.0.1",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "turbo": "2.5.6"
  }
}
